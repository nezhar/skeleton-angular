{
  "name": "star-board",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-poll": "ng serve --host 0.0.0.0 --port 8080 --poll 1000",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-start-hmr-poll": "ng serve --host 0.0.0.0 --port 8080 -c=hmr --poll 1000",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./projects/star-board/app/ -o ./projects/star-board/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./projects/star-board/tsconfig.app.json -d ./dist/docs",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.11",
    "@angular/common": "^11.2.11",
    "@angular/compiler": "^11.2.11",
    "@angular/core": "^11.2.11",
    "@angular/forms": "^11.2.11",
    "@angular/platform-browser": "^11.2.11",
    "@angular/platform-browser-dynamic": "^11.2.11",
    "@angular/router": "^11.2.11",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ng-select/ng-select": "6.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@ngxs/storage-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "@swimlane/ngx-datatable": "19.0.0",
    "@types/lodash": "^4.14.168",
    "@uirouter/angular": "8.0.1",
    "@uirouter/core": "^6.0.7",
    "@uirouter/rx": "^0.6.5",
    "bootstrap": "^4.6.0",
    "core-js": "^3.9.1",
    "date-fns": "^2.19.0",
    "lodash": "^4.17.21",
    "ngx-anx-forms": "0.3.1",
    "ngx-resource-factory": "~1.1.0",
    "ngx-toastr": "13.2.1",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.10",
    "@angular/cli": "^11.2.9",
    "@angular/compiler-cli": "^11.2.11",
    "@angular/language-service": "^11.2.11",
    "@angularclass/hmr": "^3.0.0",
    "@babel/core": "^7.13.10",
    "@compodoc/compodoc": "^1.1.9",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@types/jasmine": "~3.6.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~16.4.10",
    "babel-loader": "^8.2.2",
    "codelyzer": "~6.0.1",
    "core-js": "^3.9.1",
    "css-loader": "6.2.0",
    "git-describe": "^4.0.4",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "moment": "^2.29.1",
    "protractor": "~7.0.0",
    "puppeteer": "9.0.0",
    "ts-node": "9.1.1",
    "tslint": "~6.1.3",
    "typescript": "4.1.5"
  }
}
